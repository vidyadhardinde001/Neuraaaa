{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
    
  },
  "productName": "File Explorer",
  "mainBinaryName": "File Explorer",
  "version": "0.0.0",
  "identifier": "com.tauri.dev.test",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "file-explorer",
        "width": 1024,
        "height": 678,
        "useHttpsScheme": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}